/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.list_button{
  height:94rpx;
  background:#FFFFFF;
}
.list_button button{
  width:30%;
  height:94rpx;
  margin:0px 10%;
  background:#FFFFFF;
  display:inline-block;
  border-radius:0;
}
.active_button{
  color:#3366FF;
  border-bottom:2px solid #3366FF;
}
.default_button{
  color:#595959;
  border-bottom:2px solid #FFFFFF;
}
.list_button button:after{
  border:none;
}